{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2022", "webworker"],
    "module": "preserve",
    "moduleResolution": "bundler",
    "declaration": false,
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["dist", "test", "node_modules/**/*"]
}
